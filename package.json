{
  "name": "heleonix",
  "description": "Strict and declarative MVVM framework for web sites and web applications.",
  "author": "Heleonix - Hennadii Lutsyshyn",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel watch start:sandbox",
    "watch": "lerna exec --scope \"@heleonix/core\" -- rollup --config ../../rollup.config.js --watch",
    "start:sandbox": "lerna exec --scope \"@heleonix/sandbox\" webpack-dev-server",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json --nycrc-path=./.nycrc",
    "build": "lerna exec --scope \"@heleonix/core\" -- rollup --config ../../rollup.config.js",
    "postinstall": "lerna bootstrap --hoist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-private-methods": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/register": "7.4.4",
    "@babel/runtime-corejs3": "7.4.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "eslint": "5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-sort-class-members": "1.5.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.4.0",
    "jasmine": "3.4.0",
    "lerna": "3.13.4",
    "lint-staged": "8.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "14.1.1",
    "prettier-eslint-cli": "4.7.1",
    "rollup": "1.12.3",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "4.0.4",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
